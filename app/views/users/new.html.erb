<%= form_for @user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %><br />
  <%= f.label :email %>
  <%= f.email_field :email %><br />
  <%= f.label :password %>
  <%= f.password_field :password %><br />
  <%= f.submit :confirm %>
<% end %>
